<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
       android:shape="rectangle">
    <!-- 填充颜色 -->
    <solid android:color="@color/color_white"></solid>
    <!-- 线的宽度，颜色灰色 -->
    <stroke android:width="1dp" android:color="@color/color_white"></stroke>
    <!-- 矩形的圆角半径 -->
    <corners
        android:bottomLeftRadius="0.0dip"
        android:bottomRightRadius="5.0dip"
        android:radius="2.0dip"
        android:topLeftRadius="0.0dip"
        android:topRightRadius="5.0dip" />
</shape>
